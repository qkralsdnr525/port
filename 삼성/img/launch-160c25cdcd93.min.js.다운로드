 = Object.assign(new Event(event), params || {});\n    document.dispatchEvent(ev);\n    document.__eventlog.push(ev);\n  });\n\n\n  // run others\n  var register_the_tracker = function(tracker) {\n    Object.keys(tracker).filter(function(ev_key){ return events[ev_key] })\n      .forEach(function(ev_key) {\n        window.addEventListener(events[ev_key], (tracker[ev_key].bind(ptk)));\n      });\n  }\n\n\n\n  // register prev trackers\n  window._ptk_queue.filter(function(tracker) { return tracker.media })\n    .forEach(function(tracker){ register_the_tracker(tracker) });\n  // dispatch registration\n  dispatch_mediatrack('register');\n\n  // method overloading\n  window.ptk_media_track_register = (function(tracker) {\n    \n    if(media_registered[tracker.media]) { return; }\n\n    window._ptk_queue.push(tracker);\n    media_registered[tracker.media] = tracker;\n\n    // register event listener\n    register_the_tracker(tracker);\n\n    // forced run pre-triggered events\n    var events_done = window._ptk_queue.filter(function(pq) { return pq.event });\n    // sequencial run\n    for(var i=0; i<events_done.length; i++) {\n      var ev = events_done[i];\n      if(tracker[ev.event]) {\n        ((tracker[ev.event]).bind(ptk))(ev.params);\n      }\n    }\n  }).bind(ptk);\n\n  // default parser\n  var dispatch_on_location = function(path_pattern, ev_name, paramsFn) {\n    if(path_pattern.test(window.location.pathname)) {\n      dispatch_mediatrack(ev_name, paramsFn ? paramsFn() : undefined);\n    }\n  }\n  var scrap_itemlist_data = function(cell_selector) {\n    var rets = [];\n    document.querySelectorAll(cell_selector).forEach(function(cell){\n      rets.push({\n        id: utils.dom_text('[class$=\"-spec\"] [class$=\"-label\"]', cell),\n        name: utils.dom_text('[class$=\"-spec\"] [class$=\"-title\"]', cell).replace(/\\s+/g, ' '),\n        quantity: utils.integer(utils.dom_input({name: 'buyQty', type: 'hidden'}, cell)),\n        price: utils.integer(utils.dom_input