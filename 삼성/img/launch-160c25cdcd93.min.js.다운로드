ument.querySelector('script[src*=\"'+url+'\"]') != null; \n    },\n    integer: function(text) { return parseInt(text.replace(/[^\\d]+/g, '')) },\n    number: function(text) { return parseFloat(text.replace(/[^\\d\\.]/g, '')) },\n    dom_text: function(selector, anchor) {\n      var el = (anchor || document).querySelector(selector);\n      return el ? el.textContent.trim() : null;\n    },\n    dom_attr: function (attr_key, selector, anchor) {\n      var el = (anchor || document).querySelector(selector);\n      return el ? el[attr_key] : null;\n    },\n    dom_data: function(data_key, selector, anchor) {\n      var el = (anchor || document).querySelector(selector);\n      return el ? el.dataset[data_key] : null;\n    },\n    dom_input: function(options, anchor, inputType){\n      var selector = (inputType || 'input')\n        + Object.keys(options).map(function(ok) {\n          return '['+ok+'=\"'+options[ok]+'\"]'\n        }).join('');\n      var el = (anchor || document).querySelector(selector);\n      return el ? el.value : null;\n    },\n    set_cookie: function(key, value, expiration, domain, path) {\n      if(ptk.cookies) { ptk.cookies[key] = value; }\n      return (document.cookie = key + '=' + value \n        + ';max-age=' + (expiration || 900)\n        + ';domain=' + (domain || current_root_domain ||'.samsung.com')\n        + ';path=' + (path || '/'));\n    }\n  };\n  utils.imports = (function(url, onload, anchor) {\n    var the_script = utils.element('script', {src: url, async: true});\n    if(onload) { the_script.addEventListener('load', onload) }\n    return (anchor || window.document.head || window.document).appendChild(the_script);\n  });\n  utils.send_pixel = (function(location, params, tagname, via) {\n    var urls = utils.build_url(location, params);\n    if(via && document.querySelector(via)) {\n      document.querySelector(via).src = urls;\n      return via;\n    } else {\n      var attrs = {src: urls};\n      tagname = tagname || 'img';\n      if(